    "def resample_one_minute(df):\n",
    "    # Make sure the dataframe has a datetime index\n",
    "    df.index = pd.to_datetime(df.index)\n",
    "    # Resample the dataframe on one minute intervals\n",
    "    df_resampled = df.resample('1T').mean()\n",
    "    return df_resampled"
   ]
